let g:powerline_pycmd="py3"
set laststatus=2
imap hh <Esc>
vmap HH <Esc>
cmap HH <Esc>
map  <C-A> <Home>
imap <C-A> <Home>
cmap <C-A> <Home>
map  <C-E> <End>
imap <C-E> <End>
cmap <C-E> <End> 
nnoremap <leader>r :!%:p
syntax on
colo elflord
if has("autocmd")
  au BufReadPost * if line("'\"") > 1 && line("'\"") <= line("$") | exe "normal! g'\"" | endif
endif
